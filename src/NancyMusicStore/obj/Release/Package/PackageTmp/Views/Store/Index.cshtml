@inherits Nancy.ViewEngines.Razor.NancyRazorViewBase<List<NancyMusicStore.Models.Genre>>
    @{
        ViewBag.Title = "Store";        
    }
    <h3>Browse Genres</h3>

    <p>
        Select from  genres:
    </p>
    <ul>
        @foreach (var genre in Model)
        {
            <li><a href="/store/browse/@genre.Name">@genre.Name</a></li>
        }
    </ul>
